	<queryString>
		<![CDATA[select fa.idfactura_autoimpresor as idfac,e.razon_social as emp_razon,e.ruc as emp_ruc,e.direccion as emp_direc,e.celular as emp_cel,
t.numero as tim_nro,to_char(t.fecha_inicio,'dd/MM/yyyy') as tim_fec_ini,to_char(t.fecha_fin,'dd/MM/yyyy') as tim_fec_fin,
TRIM(TRIM(to_char(fa.cod_establecimiento,'009'))||'-'||TRIM(to_char(fa.punto_expedicion,'009'))||'-'||TRIM(to_char(fa.numeracion_secuencial,'0000009'))) as nro_factura,
cl.nombre as cli_nombre,cl.ruc as cli_ruc,cl.direccion as cli_direc,cl.telefono as cli_tel,
fa.condicion as fac_condi,to_char(fa.fecha_creado,'dd/MM/yyyy HH24:MI:ss') as fac_fecha,
fa.total_pagar as fac_tt_pagar,fa.monto_letra as fac_tt_pag_letra,fa.total_exenta as fac_tt_exe,fa.total_gravada_5 as fac_tt_g5,fa.total_gravada_10 as fac_tt_g10,
fa.liquidacion_iva_5 as fac_liq_iva5,fa.liquidacion_iva_10 as fac_liq_iva10,(fa.liquidacion_iva_5+fa.liquidacion_iva_10) as fac_tt_liq_iva,
fa.creado_por as fac_cajero,e.mensaje_final,
ifa.fk_idproducto as item_cod,ifa.cantidad as item_cant,ifa.descripcion as item_descrip,ifa.precio_venta as tim_preciov,(ifa.cantidad*ifa.precio_venta) as tim_total 
from empresa e,item_timbrado it,timbrado t,factura_autoimpresor fa,cliente cl,item_factura_auto ifa  
where e.idempresa=it.fk_idempresa 
and it.fk_idtimbrado=t.idtimbrado
and fa.fk_idtimbrado=t.idtimbrado 
and fa.fk_idcliente=cl.idcliente  
and fa.idfactura_autoimpresor=ifa.fk_idfactura_autoimpresor 
and fa.idfactura_autoimpresor=16
order by ifa.descripcion desc;]]>
